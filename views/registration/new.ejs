<div class="container">
  <div class="row">
  <div class="col">





<% if (locals.message) { %>
  <p><%= message %></p>
  <% } %>

  <form method="POST" action="/register">
    <div class="form-group1">
      <label for="username">Username</label>
      <input type="text" name="username" id="username" class="form-control1">
    </div>
    <div class="form-group1">
      <label for="email">Email</label>
      <input type="text" name="email" id="email" class="form-control1">
    </div>
    <div class="form-group1">
      <label for="password">Password</label>
      <input type="password" name="password" id="password" class="form-control1">
    </div>
    <div class="form-group1">
      <label for="passwordConfirmation">Password Confirmation</label>
      <input type="password" name="passwordConfirmation" id="passwordConfirmation" class="form-control1">
    </div>
    <button class="btn btn-primary">Register</button>
  </form>
</div>
</div>
</div>
